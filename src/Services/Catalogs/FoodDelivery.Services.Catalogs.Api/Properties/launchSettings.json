{
  "$schema": "https://json.schemaecommerce.org/launchsettings.json",
  "profiles": {
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "hotReloadProfile": "aspnetcore",
      "applicationUrl": "https://localhost:1001;http://localhost:1000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
	"http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "hotReloadProfile": "aspnetcore",
      "applicationUrl": "http://localhost:1000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "watch": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "hotReloadEnabled": true,
      "hotReloadProfile" : "aspnetcore",
      "workingDirectory": "$(ProjectDir)",
      "commandLineArgs": "watch -lp http",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "liveRecompilation": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "hotReloadProfile": "aspnetcore",
      "applicationUrl": "http://localhost:1000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
